<?xml version ="1.0" encoding="utf-8"?>
<rows>
  <page>1</page>
  <total>1</total>
  <records>1</records>
  {% for incidencia in incidencias %}
    <row id = '{{incidencia.id}}'>
      <cell> {{ incidencia.id }} </cell>
      <cell> {{ incidencia.fechaIncidencia | date('Y-m-d')  }} </cell>
      <cell> {{ incidencia.tipoIncidencia.nombre }} </cell>
      <cell> {{ incidencia.unidad }} </cell>
      <cell> {{ incidencia.estado.nombre }} </cell>
    </row>
    {% endfor %}
</rows>

