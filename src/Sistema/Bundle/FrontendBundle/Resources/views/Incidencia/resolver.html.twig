<form id="popup">
        <table border="0" cellpadding="2" cellspacing="2">
            <tr>
                <td>{{ form_label(form.id)}}</td>
                <td> {{ form_widget(form.id)}}</td>
                <td> {{ form_errors(form.id)}}</td>
            </tr>
            <tr>
                <td>{{ form_label(form.fecha_resolucion)}}</td>
                <td> {{ form_widget(form.fecha_resolucion)}}</td>
                <td> {{ form_errors(form.fecha_resolucion)}}</td>
            </tr>
            <tr>
                <td>{{ form_label(form.solucion)}}</td>
                <td> {{ form_widget(form.solucion)}}</td>
                <td> {{ form_errors(form.solucion)}}</td>
            </tr>
            <tr>
                <td>{{ form_label(form.estado)}}</td>
                <td> {{ form_widget(form.estado)}}</td>
                <td> {{ form_errors(form.estado)}}</td>
            </tr>
            
         
            <tr>
                <td align="center" >&nbsp; {{ form_rest(form)}}</td>
            </tr>
            <tr>
                <td colspan="3">
                    <div class="resultados ui-state-default ui-corner-all hide" style="font-size:11px; padding: 4px 4px 4px 4px; height:20px; width:240px"></div>
                </td>
            </tr>
            <tr>
                <td width="100%" colspan="2" >
                    <div class="ui-widget-content">
                        <div class="divBtns">
                            <table width="100%">
                                <tr>
                                    <td align="left">
                                        <button id="resolver-incidencia">Resolver</button>&nbsp;
                                    </td>
				</tr>
                            </table>
                        </div>
                    </div>
                </td>
            </tr>
        </table>
        </form>    
              

